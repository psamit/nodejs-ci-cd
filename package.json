{
  "name": "nodejs-ci-cd",
  "version": "1.0.0",
  "description": "test ci with github",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "tsc",
    "dev": "ts-node-dev --respawn ./application/server.ts",
    "prod": "npm run build && node ./build/server.js",
    "test": "jest --config jest.config.json",
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc  **/*.{js,json,ts} --write",
    "format": "npm run prettier-format && npm run lint --fix"
  },
  "author": "Amit Suman",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.3",
    "prettier": "^2.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/jest": "^27.4.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "jest": "^27.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^2.0.0-0",
    "typescript": "^4.5.5"
  }
}
